import random
password = []
schar = ['@','#','$','%','&','*','Â£','1','2','3','4','5','6','7','8','9','0']
colour = input("What's your favourite colour? (one word): ")
place = input("What's your favourite place? (one word): ")
animal = input("What's you favourite animal? (one word): ")
n = random.randint(1,7)
password += colour
password += place
password += animal
for i in range(n):
    password += random.choice(schar)
print(f"Password: {''.join(password)}")
